<?xml version="1.0" encoding="utf-8"?>
<root>
  <shell>
    <default>
      <property name="application.path" value="C:\Program Files\Neovim\bin\nvim-qt.exe" />
    </default>
    <menu name="Edit with nvim-qt">
      <visibility maxfolders="0" exists="${application.path}" />
      <actions>
        <exec path="${application.path}" arguments="-- &quot;${selection.path}&quot;" />
      </actions>
    </menu>
  </shell>
</root>
